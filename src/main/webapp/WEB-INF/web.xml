<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>DoctorController</servlet-name>
  	<display-name>DoctorController</display-name>
  	<description></description>
  	<servlet-class>abc.laboratory.webapp.Controller.DoctorController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>PatientController</servlet-name>
  	<display-name>PatientController</display-name>
  	<description></description>
  	<servlet-class>abc.laboratory.webapp.Controller.PatientController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>DoctorController</servlet-name>
  	<url-pattern>/DoctorController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>PatientController</servlet-name>
  	<url-pattern>/PatientController</url-pattern>
  </servlet-mapping>
  
</web-app>
